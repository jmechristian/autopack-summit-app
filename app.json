{
  "expo": {
    "name": "autopack-summit-app",
    "slug": "autopack-summit-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "autopacksummitapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jmepackschool.autopacksummitapp",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We use your photo library so you can select and upload a profile picture.",
        "NSCameraUsageDescription": "We use the camera to let you capture a profile picture.",
        "NSPhotoLibraryAddUsageDescription": "We need access to save your uploaded or captured profile picture.",
        "NSContactsUsageDescription": "We use your contacts to save scanned attendee contact details to your phone.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jmepackschool.autopacksummitapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a28f9527-bfd5-49ae-b62c-a4ce791bc274",
        "owner": "the-packaging-school"
      }
    }
  }
}
